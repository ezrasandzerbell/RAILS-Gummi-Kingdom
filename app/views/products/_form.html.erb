<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost_per_pound %>
  <%= f.number_field :cost %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit  %>
<% end %>
<br><br>
